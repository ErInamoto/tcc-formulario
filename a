id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
id_tcc INT NOT NULL,
nome VARCHAR NOT NULL,
id_curso NOT NULL,
modulo INT NOT NULL,
apresentacao INT NOT NULL,
stand INT NOT NULL,
pratico VARCHAR(1) NOT NULL,
viavel VARCHAR(1) NOT NULL,
favoravel VARCHAR(1) NOT NULL,
tecnologico VARCHAR(1) NOT NULL,
positivo VARCHAR,
incompleto VARCHAR(1) NOT NULL,
inviavel VARCHAR(1) NOT NULL,
caro VARCHAR(1) NOT NULL,
critica VARCHAR,
inclusivo VARCHAR(1) NOT NULL,
sustentavel VARCHAR(1) NOT NULL,
startup VARCHAR(1) NOT NULL,
comunicacao INT NOT NULL,
escrita INT NOT NULL,
avaliacao INT NOT NULL,
coment_tcc VARCHAR,
coment_mostra VARCHAR,